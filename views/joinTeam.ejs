<%- include("partials/header"); -%>
</body>
<img class="create_team_logo" src="/images/join.png" alt="">
<form class="inputForm" action="/<%= userId %>/joinTeam" method="post">
  <p class="create_team_msg">Enter Team ID Below to join:</p>
  <input type="text" class="form-control" id="teamIDInput" name="teamID" placeholder="Team ID">
  <button class="button_design btn btn-primary" type="submit" name="submitButton">JOIN</button>
</form>
<% if(joinStatus != ''){%>
  <div class="joinStatusMessage"><p><%= joinStatus %></p></div>
<% } %>
<link rel="stylesheet" href="/css/create_style.css">
</html>
